vector <int> remove_duplicates(vector<int> arr) {
  vector<int> no_duplicates_arr;
  for(int i = 0; i < arr.size(); i++){
    if(find(no_duplicates_arr.begin(), no_duplicates_arr.end(), arr[i]) == no_duplicates_arr.end())
      no_dupliacates_arr.push_back(arr[i]);
  return no_duplicates_arr;
}
