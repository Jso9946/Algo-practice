//apparently smths wrong w my first attempt

char first_repeating_character (string str) {
  for (int i = 0; i < str.length(); i++) {
    for(int j = 0; j < str.length(); j++) {
      if (str.at(i) == str.at(j)){
        return str.at(i);
    }
  }
  return '\0';
}
